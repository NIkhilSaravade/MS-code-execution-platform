INSERT INTO test_case (problem_id, input, expected_output, hidden) VALUES
(10, '[1,8,6,2,5,4,8,3,7]', '49', false),
(10, '[1,1]', '1', false),
(10, '[4,3,2,1,4]', '16', true);




SELECT p.name, t.input, t.expected_output
FROM problem p
JOIN test_case t ON p.id = t.problem_id;














