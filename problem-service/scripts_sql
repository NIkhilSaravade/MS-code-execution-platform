INSERT INTO problem (name, description, constraints) VALUES
('Two Sum', 'Find two numbers that add up to a target.', '2 <= nums.length <= 10^4'),
('Best Time to Buy and Sell Stock', 'Maximize profit from stock prices.', '1 <= prices.length <= 10^5'),
('Contains Duplicate', 'Check if array contains duplicates.', '1 <= nums.length <= 10^5'),
('Product of Array Except Self', 'Return array of products except self.', '2 <= nums.length <= 10^5'),
('Maximum Subarray', 'Find contiguous subarray with maximum sum.', '-10^4 <= nums[i] <= 10^4'),
('Maximum Product Subarray', 'Find contiguous subarray with max product.', '-10 <= nums[i] <= 10'),
('Find Minimum in Rotated Sorted Array', 'Find minimum element in rotated sorted array.', '1 <= nums.length <= 5000'),
('Search in Rotated Sorted Array', 'Search target in rotated sorted array.', '-10^4 <= nums[i] <= 10^4'),
('3Sum', 'Find all unique triplets summing to zero.', '-10^5 <= nums[i] <= 10^5'),
('Container With Most Water', 'Find max water container area.', '2 <= height.length <= 10^5');


